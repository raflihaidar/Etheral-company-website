{
  "name": "ethereal-web",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/user/**",
      "use": "@vercel/static"
    },
    {
      "src": "client/admin/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.js"
    }
  ]
}
